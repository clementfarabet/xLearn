# Find the Lua includes and library
#
# LUA_INCLUDE_DIR -- where to find the includes
# LUA_LIBRARIES -- list of libraries to link against
# LUA_EXECUTABLE -- lua executable
# LUAC_EXECUTABLE -- luac executable
# LUA_FOUND -- set to 1 if found

SET(LUA_FOUND 1)
SET(LUA_INCLUDE_DIR "@LUA_INCLUDE_DIR@")
SET(LUA_LIBRARIES "@LUA_LIBRARIES@")
SET(LUA_READLINE_LIBRARIES "@LUA_READLINE_LIBRARIES@")
SET(LUA_EXECUTABLE "@LUA_EXECUTABLE@")
SET(LUAC_EXECUTABLE "@LUAC_EXECUTABLE@")
SET(LUA_PACKAGE_CPATH "@LUA_PACKAGE_CPATH@")
SET(LUA_PACKAGE_PATH "@LUA_PACKAGE_PATH@")
